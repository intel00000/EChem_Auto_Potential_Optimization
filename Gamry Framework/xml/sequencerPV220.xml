    <element>
      <name>Sampled DC Voltammetry</name>
      <classname>SAMPLEDDC</classname>
      <type>normal</type>
      <description>Performs a Sampled DC Voltammetry experiment</description>
      <parameters>
        <explain_label tag="TITLE" value="Sampled DC Voltammetry" desc="Test Identifier" />
        <explain_output tag="OUTPUT" value="DCV.DTA" desc="Output File" />
        <explain_notes tag="NOTES" value="" desc="Notes..." />
        <explain_quant tag="VINIT" value="0.0" desc="Initial E (V)" />
        <explain_quant tag="VFINAL" value="-1.0" desc="Final E (V)" />
        <explain_quant tag="STEPSIZE" value="2.0" desc="Step Size (mV)" />
        <explain_quant tag="SAMPLETIME" value="1.0" desc="Sample Period (s)" />
        <explain_toggle tag="NOISEREJECT" checked="TRUE" desc="Noise Rejection" />
        <explain_selector tag="IMODE" index="1" desc="I/E Range Mode" item0="Auto" item1="Fixed" />
        <explain_quant tag="IRANGE" value="0.3" desc="Max Current (mA)" />
        <explain_quant tag="AREA" value="1.0" desc="Sample Area (cm^2)" />
        <explain_iquant tag="EQDELAY" value="5" desc="Equil. Time (s)" />
        <explain_selector tag="IRCOMP" index="0" desc="IRComp" item0="None" item1="PF" />
        <explain_quant tag="PFCOR" value="50.0" desc="PF Corr. (ohm)" />
      </parameters>
      <usecount>0</usecount>
    </element>
