    <element>
      <name>Expected Max V</name>
      <classname>EXPECTEDMAXV</classname>
      <type>normal</type>
      <description>Check if High Voltage Electrometer can be used</description>
      <parameters>
        <explain_quant tag="EXPECTEDMAXV" value="10.0" desc="Expected Max V (V)" />
      </parameters>
      <usecount>0</usecount>
    </element>
    <element>
      <name>Chronoamperometry</name>
      <classname>CHRONOAMP</classname>
      <type>normal</type>
      <description>Performs a chronoamperometry measurement under potentiostatic control</description>
      <parameters>
        <explain_label tag="TITLE" value="Chronoamperometry Scan" desc="Test Identifier" />
        <explain_output tag="OUTPUT" value="CHRONOA.DTA" desc="Output File" />
        <explain_notes tag="NOTES" value="" desc="Notes..." />
        <explain_poten tag="VPRESTEP" value="0.0" versus="0" desc="Pre-step Voltage (V)" />
        <explain_quant tag="TPRESTEP" value="0.5" desc="Pre-step Delay Time (s)" />
        <explain_poten tag="VSTEP1" value="0.5" versus="0" desc="Step 1 Voltage (V)" />
        <explain_quant tag="TSTEP1" value="5.0" desc="Step 1 Time (s)" />
        <explain_poten tag="VSTEP2" value="0.1" versus="0" desc="Step 2 Voltage (V)" />
        <explain_quant tag="TSTEP2" value="5.0" desc="Step 2 Time (s)" />
        <explain_quant tag="SAMPLETIME" value="0.01" desc="Sample Period (s)" />
        <explain_quant tag="ILIMIT" value="10.0" desc="Limit I (mA/cm^2)" />
        <explain_toggle tag="DECIMATE" checked="FALSE" desc="Decimate" />
        <explain_quant tag="AREA" value="1.0" desc="Sample Area (cm^2)" />
        <explain_selector tag="IMODE" index="0" desc="I/E Range Mode" item0="Auto" item1="Fixed" />
        <explain_selector tag="SAMPLINGMODE" index="1" desc="Sampling Mode" item0="Fast" item1="Noise Reject" item2="Surface" />
        <explain_quant tag="IRANGE" value="0.3" desc="Max Current (mA)" />
        <explain_quant tag="EQDELAY" value="0.0" desc="Equil. Time (s)" />
        <explain_selector tag="IRCOMP" index="0" desc="IRComp" item0="None" item1="PF" item2="CI" />
        <explain_quant tag="PFCOR" value="50.0" desc="PF Corr. (ohm)" />
      </parameters>
      <usecount>0</usecount>
    </element>
